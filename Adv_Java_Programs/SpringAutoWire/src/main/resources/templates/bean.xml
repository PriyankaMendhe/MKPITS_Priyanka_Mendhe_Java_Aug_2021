<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="eob" class="com.example.springautowire.Employee">

        <constructor-arg index="1" value="priyanka"/>
         <constructor-arg  index="0" value="1"/>
    </bean>

    <bean id="rob" class="com.example.springautowire.Report">
        <property name="employeeList">

            <list>
                 <ref bean="eob"></ref>

                <bean class="com.example.springautowire.Employee">

                    <constructor-arg index="1" value="priyanka1"/>
                    <constructor-arg  index="0" value="11"/>
                </bean>

                <bean class="com.example.springautowire.Employee">

                    <constructor-arg index="1" value="priyanka12"/>
                    <constructor-arg  index="0" value="111"/>
                </bean>


            </list>
        </property>




    </bean>

<!--    <bean id="acc1" autowire="constructor" class="com.example.springautowire.Account">-->

<!--    </bean>-->

</beans>