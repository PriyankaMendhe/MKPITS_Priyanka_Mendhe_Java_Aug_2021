<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
 <bean id="eob" class="com.example.springpostprocessorex.Vehicle">
      <property name="id" value="1"></property>
 </bean>

    <bean id="derivedVehicle" parent="eob">
        <property name="name" value="KIA"></property><property name="price" value="500000"></property>


    </bean>

    <bean class="com.example.springpostprocessorex.PostProcessorCheck"></bean>
</beans>